import json
with open('F:\\Onkar\\data1.json') as f:
	x = json.load(f)
print("Type of Json object: ", type(x))
for name in x:
    print("Name: ", name)
    print("Phone Number: ", x[name]["number"])
    print("Age: ", x[name]["age"])
    
    print("Address:")
    for line in x[name]["address"]:
        print(line)
    print()
